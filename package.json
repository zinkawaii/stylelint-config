{
  "name": "@zinkawaii/stylelint-config",
  "type": "module",
  "version": "0.3.4",
  "packageManager": "pnpm@10.28.1",
  "description": "Utakata na Shiawase",
  "author": "KazariEX",
  "license": "MIT",
  "repository": "zinkawaii/stylelint-config",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "prepack": "pnpm run build",
    "release": "bumpp --no-push -c \"release: v%s\"",
    "test": "vitest"
  },
  "dependencies": {
    "@stylistic/stylelint-plugin": "catalog:",
    "stylelint-config-html": "catalog:",
    "stylelint-config-standard-scss": "catalog:",
    "stylelint-order": "catalog:"
  },
  "devDependencies": {
    "@zinkawaii/eslint-config": "catalog:",
    "@zinkawaii/tsconfig": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "known-css-properties": "catalog:",
    "stylelint": "catalog:",
    "tsdown": "catalog:",
    "vitest": "catalog:"
  }
}
